<!DOCTYPE html>
<html lang="zh-CN">

<head>
    {{@includes/head.niml}}
    <link type="text/css" rel="stylesheet" href="{{$__AF_SRCDIR}}Interblocks/see/ListView/style.css">
    <link href="{{$__SRCDIR}}stylesheets/detail.css" type="text/css" rel="stylesheet">
</head>

<body>
    {{@includes/header.niml}}
    <!-- main slider carousel -->
    <section class="ic section bg-light">
        <div class="ic blank-30"></div>
        <div class="ic w1200 pa-20 bg-white grid-21" style=" box-shadow: 0px 0px 2px #DDD">
            <div class="nm-15 main-container">
                <div class="ic blank-10"></div>
                <header><span class="dir">
                    <a href="{{$REQUEST.REST_HANDLER_DIR}}g/about/article/index.html">学院概况</a>
                    <i>/</i><a href="{{$REQUEST.REST_HANDLER_DIR}}s/photos/">建筑印象</a>
                    </span>
                </header>
                <article class="news-content">
                    <header>{{$___TITLE}}</header>
                    <div class="news-desc">
                        <span>{{$___PUBTIME}}</span>
                        <span>编辑：{{$___AUTHOR}}</span>
                        <span>来源：<if $___SOURCE>{{$___SOURCE}}<else />本站</if></span>
                        <span>查看：{{$___KEY_COUNT}}<span>
                    </div>
                    <decode value="$___CONTENT" />
                    <ul class="ic listview page-list train">
                    <def name="$pages " onload="Library.ect.Pager.getPageListDataByCount " args="9" />
                    <li class="list-item" onclick="window.location.href='?page={{$pages[`f`]}}'">&lt;&lt;</li>
                    <if $pages[`f`] lt $___Cpage>
                        <li class="list-item" onclick="window.location.href='?page={{$pages[`p`]}}'">&lt;</li>
                    </if>
                    {{$length =$pages[`length`]}}
                    <for $index in [0, $length)>
                        <if $pages[$index] eq $___Cpage>
                            <li class="list-item actived" onclick="window.location.href='?page={{$pages[$index]}}'">{{$pages[$index]}}</li>
                            <else />
                            <li class="list-item" onclick="window.location.href='?page={{$pages[$index]}}'">{{$pages[$index]}}</li>
                        </if>
                    </for>
                    <if $pages[`l`] gt $___Cpage>
                        <li class="list-item" onclick="window.location.href='?page={{$pages[`n`]}}'">&gt;</li>
                    </if>
                    <li class="list-item" onclick="window.location.href='?page={{$pages[`l`]}}'">&gt;&gt;</li>
                </ul>
                </article>
                <div class="ic h50 al-center title-50 ft-999 ft-12 grid-12">
                    <div class="nm-6">上一篇：
                        <if $___Earlier_InSameCategory>
                            <a href="{{$REQUEST.REST_HANDLER_DIR}}s/{{$___SET_ALIAS}}/article/{{***$___Earlier_InSameCategory.ID***}}/">
                                <str value="$___Earlier_InSameCategory.TITLE" length="19" />
                            </a>
                            <else /> 没有了 </if>
                    </div>
                    <div class="nm-6">下一篇：
                        <if $___Later_InSameCategory>
                            <a href="{{$REQUEST.REST_HANDLER_DIR}}s/{{$___SET_ALIAS}}/article/{{***$___Later_InSameCategory.ID***}}/">
                                <str value="$___Later_InSameCategory.TITLE" length="19" />
                            </a>
                            <else /> 没有了 </if>
                    </div>
                </div>
            </div>
            <div class="nm-1">&nbsp;</div>
            <div class="nm-5">
                <div class="side-list-title">栏目导航</div>
                <ul class="side-list">
                    <li class=""><a href="{{$REQUEST.REST_HANDLER_DIR}}g/about/article/index.html">学院简介</a></li>
                    <li class=""><a href="{{$REQUEST.REST_HANDLER_DIR}}s/leadership/">现任领导</a></li>
                    <li class=""><a href="{{$REQUEST.REST_HANDLER_DIR}}s/teachers/">教师风采</a></li>
                    <li class=""><a href="{{$REQUEST.REST_HANDLER_DIR}}g/about/article/majors.html">专业介绍</a></li>
                    <li class="actived"><a href="{{$REQUEST.REST_HANDLER_DIR}}s/photos/">建筑印象</a></li>
                </ul>
            </div>
        </div>
        <div class="ic blank-50"></div>
    </section>
    {{@includes/footer.niml}}
</body>

</html>